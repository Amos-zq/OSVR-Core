
#ifndef @INCLUDE_GUARD_NAME@
#define @INCLUDE_GUARD_NAME@

#ifdef @STATIC_DEFINE@
#  define @EXPORT_MACRO_NAME@
#  define @NO_EXPORT_MACRO_NAME@
#endif

/* Per-platform defines */
#if defined(_MSC_VER)
/* MSVC on Windows */

#ifndef @EXPORT_MACRO_NAME@
#  ifdef @EXPORT_IMPORT_CONDITION@
      /* We are building this library */
#    define @EXPORT_MACRO_NAME@ __declspec(dllexport)
#  else
      /* We are using this library */
#    define @EXPORT_MACRO_NAME@ __declspec(dllimport)
#  endif
#endif

#ifndef @DEPRECATED_MACRO_NAME@
#  define @DEPRECATED_MACRO_NAME@ __declspec(deprecated)
#endif
    
#elif defined(_WIN32) && defined(_GNUC_)
/* GCC-compatible on Windows */

#ifndef @EXPORT_MACRO_NAME@
#  ifdef @EXPORT_IMPORT_CONDITION@
      /* We are building this library */
#    define @EXPORT_MACRO_NAME@ __attribute__((dllexport))
#  else
      /* We are using this library */
#    define @EXPORT_MACRO_NAME@ __attribute__((dllimport))
#  endif
#endif

#ifndef @NO_EXPORT_MACRO_NAME@
#  define @NO_EXPORT_MACRO_NAME@ __attribute__((visibility(\"hidden\")))
#endif

#ifndef @DEPRECATED_MACRO_NAME@
#  define @DEPRECATED_MACRO_NAME@ __attribute__ ((__deprecated__))"
#endif

#elif (defined(_GNUC_) && _GNUC_C >= 4) || defined(__APPLE__) ||               \
    defined(__MACOSX__)
/* *Nix, Android or Mac OS */

#ifndef @EXPORT_MACRO_NAME@
  /* We are building/using this library */
  #define @EXPORT_MACRO_NAME@ __attribute__((visibility(\"default\")))
#endif

#ifndef @NO_EXPORT_MACRO_NAME@
#  define @NO_EXPORT_MACRO_NAME@ __attribute__((visibility(\"hidden\")))
#endif

#ifndef @DEPRECATED_MACRO_NAME@
#  define @DEPRECATED_MACRO_NAME@ __attribute__ ((__deprecated__))
#endif

#endif
/* End of platform ifdefs */

/* fallback def */
#ifndef @EXPORT_MACRO_NAME@
#  define @EXPORT_MACRO_NAME@
#endif

/* fallback def */
#ifndef @NO_EXPORT_MACRO_NAME@
#  define @NO_EXPORT_MACRO_NAME@ @DEFINE_NO_EXPORT@
#endif

/* fallback def */
#ifndef @DEPRECATED_MACRO_NAME@_EXPORT
#  define @DEPRECATED_MACRO_NAME@_EXPORT @EXPORT_MACRO_NAME@ @DEPRECATED_MACRO_NAME@
#endif

/* fallback def */
#ifndef @DEPRECATED_MACRO_NAME@_NO_EXPORT
#  define @DEPRECATED_MACRO_NAME@_NO_EXPORT @NO_EXPORT_MACRO_NAME@ @DEPRECATED_MACRO_NAME@
#endif

#endif
